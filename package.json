{
  "name": "instagram-desktop",
  "version": "0.1.0",
  "description": "instagram desktop client",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "package":
      "electron-packager . instagram --all --out=./out --overwrite --icon=./instagram.ico",
    "package:mac":
      "electron-packager . Instagram --mac --out=./out --overwrite --icon=./instagram.ico"
  },
  "author": "Maxim Filimonov",
  "license": "MIT",
  "homepage": "https://instagram.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atom/electron-quick-start.git"
  },
  "dependencies": {
    "electron": "^1.8.2",
    "electron-packager": "^11.0.1"
  }
}
